{
    "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
    "version": 1,
    "newProjectRoot": "projects",
    "projects": {
        "elearning-ng": {
            "root": "",
            "sourceRoot": "src",
            "projectType": "application",
            "architect": {
                "lint": {
                    "builder": "@angular-eslint/builder:lint",
                    "options": {
                        "lintFilePatterns": ["src/**/*.ts", "src/**/*.html"]
                    }
                },
                "build": {
                    "builder": "@angular-devkit/build-angular:browser",
                    "options": {
                        "aot": false,
                        "outputPath": "dist",
                        "resourcesOutputPath": "dist-resources",
                        "index": "src/index.html",
                        "main": "src/main.ts",
                        "tsConfig": "src/tsconfig.app.json",
                        "stylePreprocessorOptions": {
                            "includePaths": ["."]
                        },
                        "allowedCommonJsDependencies": [
                            "detectrtc",
                            "recordrtc",
                            "wavesurfer.js",
                            "sweetalert2"
                        ],
                        "polyfills": "src/polyfills.ts",
                        "assets": ["src/assets", "src/.htaccess", "src/favicon.ico"],
                        "styles": [
                            "node_modules/ng-drag-drop/style.css",
                            "src/styles.scss",
                            "node_modules/simple-keyboard/build/css/index.css"
                        ],
                        "scripts": [
                            "node_modules/jquery/dist/jquery.min.js",
                            "node_modules/bootstrap/dist/js/bootstrap.bundle.min.js",
                            "node_modules/wavesurfer.js/dist/wavesurfer.min.js",
                            "node_modules/wavesurfer.js/dist/plugin/wavesurfer.microphone.min.js",
                            "node_modules/bootbox/bootbox.js"
                        ],
                        "vendorChunk": true,
                        "extractLicenses": false,
                        "buildOptimizer": false,
                        "sourceMap": true,
                        "optimization": false,
                        "namedChunks": true
                    },
                    "configurations": {
                        "production": {
                            "budgets": [
                                {
                                    "type": "anyComponentStyle",
                                    "maximumWarning": "6kb"
                                }
                            ],
                            "optimization": true,
                            "outputHashing": "all",
                            "sourceMap": false,
                            "namedChunks": false,
                            "aot": true,
                            "extractLicenses": true,
                            "vendorChunk": false,
                            "buildOptimizer": true,
                            "fileReplacements": []
                        },
                        "staging": {
                            "budgets": [
                                {
                                    "type": "anyComponentStyle",
                                    "maximumWarning": "6kb"
                                }
                            ],
                            "optimization": true,
                            "outputHashing": "all",
                            "sourceMap": false,
                            "namedChunks": false,
                            "aot": true,
                            "extractLicenses": true,
                            "vendorChunk": false,
                            "buildOptimizer": true,
                            "fileReplacements": []
                        },
                        "local": {
                            "budgets": [
                                {
                                    "type": "anyComponentStyle",
                                    "maximumWarning": "6kb"
                                }
                            ],
                            "outputHashing": "all",
                            "namedChunks": false,
                            "sourceMap": true,
                            "extractLicenses": true,
                            "vendorChunk": false,
                            "fileReplacements": []
                        },
                        "demo": {
                            "budgets": [
                                {
                                    "type": "anyComponentStyle",
                                    "maximumWarning": "6kb"
                                }
                            ],
                            "outputHashing": "all",
                            "sourceMap": true,
                            "namedChunks": false,
                            "aot": false,
                            "extractLicenses": true,
                            "vendorChunk": false,
                            "buildOptimizer": false,
                            "fileReplacements": []
                        }
                    }
                },
                "serve": {
                    "builder": "@angular-devkit/build-angular:dev-server",
                    "options": {
                        "browserTarget": "elearning-ng:build"
                    },
                    "configurations": {
                        "production": {
                            "browserTarget": "elearning-ng:build:production"
                        },
                        "staging": {
                            "browserTarget": "elearning-ng:build:staging"
                        },
                        "local": {
                            "browserTarget": "elearning-ng:build:local"
                        },
                        "demo": {
                            "browserTarget": "elearning-ng:build:demo"
                        }
                    }
                },
                "test": {
                    "builder": "@angular-devkit/build-angular:karma",
                    "options": {
                        "karmaConfig": "./karma.conf.js",
                        "polyfills": ["src/polyfills.ts", "zone.js", "zone.js/testing"],
                        "tsConfig": "src/tsconfig.spec.json",
                        "scripts": [
                            "node_modules/jquery/dist/jquery.min.js",
                            "node_modules/bootstrap/dist/js/bootstrap.bundle.min.js",
                            "node_modules/wavesurfer.js/dist/wavesurfer.min.js",
                            "node_modules/wavesurfer.js/dist/plugin/wavesurfer.microphone.min.js",
                            "node_modules/bootbox/bootbox.js"
                        ],
                        "styles": ["node_modules/ng-drag-drop/style.css", "src/styles.scss"],
                        "assets": ["src/assets", "src/.htaccess", "src/favicon.ico"]
                    }
                },
                "cypress-run": {
                    "builder": "@cypress/schematic:cypress",
                    "options": {
                        "devServerTarget": "elearning-ng:serve"
                    },
                    "configurations": {
                        "production": {
                            "devServerTarget": "elearning-ng:serve:production"
                        }
                    }
                },
                "cypress-open": {
                    "builder": "@cypress/schematic:cypress",
                    "options": {
                        "watch": true,
                        "headless": false
                    }
                },
                "ct": {
                    "builder": "@cypress/schematic:cypress",
                    "options": {
                        "devServerTarget": "elearning-ng:serve",
                        "watch": true,
                        "headless": false,
                        "testingType": "component"
                    },
                    "configurations": {
                        "development": {
                            "devServerTarget": "elearning-ng:serve:development"
                        }
                    }
                },
                "e2e": {
                    "builder": "@cypress/schematic:cypress",
                    "options": {
                        "devServerTarget": "elearning-ng:serve",
                        "watch": true,
                        "headless": false
                    },
                    "configurations": {
                        "production": {
                            "devServerTarget": "elearning-ng:serve:production"
                        }
                    }
                }
            }
        },
        "elearning-ng-e2e": {
            "root": "e2e",
            "sourceRoot": "e2e",
            "projectType": "application",
            "architect": {
                "e2e": {
                    "builder": "@cypress/schematic:cypress",
                    "options": {
                        "devServerTarget": "elearning-ng-e2e:serve",
                        "watch": true,
                        "headless": false
                    },
                    "configurations": {
                        "production": {
                            "devServerTarget": "elearning-ng-e2e:serve:production"
                        }
                    }
                },
                "cypress-run": {
                    "builder": "@cypress/schematic:cypress",
                    "options": {
                        "devServerTarget": "elearning-ng-e2e:serve",
                        "configFile": "e2e/cypress.config.js"
                    },
                    "configurations": {
                        "production": {
                            "devServerTarget": "elearning-ng-e2e:serve:production"
                        }
                    }
                },
                "cypress-open": {
                    "builder": "@cypress/schematic:cypress",
                    "options": {
                        "watch": true,
                        "headless": false,
                        "configFile": "e2e/cypress.config.js"
                    }
                },
                "ct": {
                    "builder": "@cypress/schematic:cypress",
                    "options": {
                        "devServerTarget": "elearning-ng-e2e:serve",
                        "watch": true,
                        "headless": false,
                        "testingType": "component"
                    },
                    "configurations": {
                        "development": {
                            "devServerTarget": "elearning-ng-e2e:serve:development"
                        }
                    }
                }
            }
        }
    },
    "schematics": {
        "@schematics/angular:component": {
            "prefix": "app",
            "style": "scss"
        },
        "@schematics/angular:directive": {
            "prefix": "app"
        }
    },
    "cli": {
        "schematicCollections": ["@cypress/schematic", "@schematics/angular"],
        "analytics": false
    }
}
